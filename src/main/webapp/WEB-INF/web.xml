<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>Project2</display-name>
    <context-param>
        <param-name>dbURL</param-name>
        <param-value><![CDATA[${sqliteDatabaseUrl}]]></param-value>
    </context-param>
    <context-param>
        <param-name>ticketmasterApiURL</param-name>
        <param-value>https://app.ticketmaster.com/discovery/v2/events</param-value>
    </context-param>
    <context-param>
        <param-name>ticketmasterApiToken</param-name>
        <param-value><![CDATA[${ticketmasterApiToken}]]></param-value>
    </context-param>
    <context-param>
        <param-name>autocompleteCandidateLimit</param-name>
        <param-value>20</param-value>
    </context-param>
    <servlet>
        <servlet-name>SignUpServlet</servlet-name>
        <servlet-class>csci310.servlets.SignUpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LogInServlet</servlet-name>
        <servlet-class>csci310.servlets.LogInServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogInServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TmApiProxyServlet</servlet-name>
        <servlet-class>csci310.servlets.TmApiProxyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TmApiProxyServlet</servlet-name>
        <url-pattern>/api/tm-api-proxy</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CreateProposalServlet</servlet-name>
        <servlet-class>csci310.servlets.CreateProposalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateProposalServlet</servlet-name>
        <url-pattern>/create-proposal</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BlockListServlet</servlet-name>
        <servlet-class>csci310.servlets.BlockListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BlockListServlet</servlet-name>
        <url-pattern>/block-list</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AutocompleteServlet</servlet-name>
        <servlet-class>csci310.servlets.AutocompleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AutocompleteServlet</servlet-name>
        <url-pattern>/api/usernames</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProposalDetailsServlet</servlet-name>
        <servlet-class>csci310.servlets.ProposalDetailsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProposalDetailsServlet</servlet-name>
        <url-pattern>/proposal</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EventVoteServlet</servlet-name>
        <servlet-class>csci310.servlets.EventVoteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EventVoteServlet</servlet-name>
        <url-pattern>/api/event-vote</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteEventServlet</servlet-name>
        <servlet-class>csci310.servlets.DeleteEventServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteEventServlet</servlet-name>
        <url-pattern>/api/delete-event</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteProposalServlet</servlet-name>
        <servlet-class>csci310.servlets.DeleteProposalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteProposalServlet</servlet-name>
        <url-pattern>/api/delete-proposal</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AcceptProposalServlet</servlet-name>
        <servlet-class>csci310.servlets.AcceptProposalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AcceptProposalServlet</servlet-name>
        <url-pattern>/api/accept-proposal</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteInvitedUserServlet</servlet-name>
        <servlet-class>csci310.servlets.DeleteInvitedUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteInvitedUserServlet</servlet-name>
        <url-pattern>/api/delete-invited-user</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>HideProposalServlet</servlet-name>
        <servlet-class>csci310.servlets.HideProposalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HideProposalServlet</servlet-name>
        <url-pattern>/api/hide-proposal</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProposalListServlet</servlet-name>
        <servlet-class>csci310.servlets.ProposalListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProposalListServlet</servlet-name>
        <url-pattern></url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FinalizeProposalServlet</servlet-name>
        <servlet-class>csci310.servlets.FinalizeProposalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FinalizeProposalServlet</servlet-name>
        <url-pattern>/api/finalize-proposal</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProposalDraftServlet</servlet-name>
        <servlet-class>csci310.servlets.ProposalDraftServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProposalDraftServlet</servlet-name>
        <url-pattern>/api/proposal-draft</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PostProposalServlet</servlet-name>
        <servlet-class>csci310.servlets.PostProposalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PostProposalServlet</servlet-name>
        <url-pattern>/api/post-proposal</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LogOutServlet</servlet-name>
        <servlet-class>csci310.servlets.LogOutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogOutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
</web-app>